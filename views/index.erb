<script type="text/javascript" src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.3.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var slideImages = [$("#slideshow0"), $("#slideshow1")];
        var imageIndex = 0;
        var nextSlide = 2;  //0 and 1 are initial values
        var slideWrap = 67; // There are 66 slides
        function advanceSlide() {
            var oldImage = slideImages[imageIndex];
            imageIndex = ++imageIndex % slideImages.length;
            var newImage = slideImages[imageIndex];

            // Fade old image
            var closeOver = nextSlide;
            oldImage.fadeTo(800, 0, function () {
                $(this).attr("src", "slideshow/" + closeOver + ".jpg");
            });

            // Fade in new image
            newImage.fadeTo(800, 1);

            // Advance slide
            nextSlide = ++nextSlide % slideWrap;
        }
        window.setInterval(advanceSlide, 8000);
    });
</script>
